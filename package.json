{
  "name": "@otohime-site/parser",
  "version": "24.1.0",
  "license": "MIT",
  "type": "module",
  "types": "index.d.ts",
  "homepage": "https://github.com/otohime-site/parser#readme",
  "exports": {
    ".": "./index.js",
    "./dx_intl": "./dx_intl/index.js",
    "./dx_intl/*": "./dx_intl/*.js"
  },
  "scripts": {
    "lint": "eslint",
